{
    "name": "edibotopic site",
    "version": "1.0.0",
    "description": "Personal website for blogging and projects.",
    "main": ".eleventy.js",
    "site-url": "https://edibotopic.github.io/edibotopic-website",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/edibotopic/edibotopic-website.github.io.git"
    },
    "keywords": [
        "food",
        "science",
        "technology",
        "philosophy",
        "blog"
    ],
    "author": "Shane Crowley <edibotopic@gmail.com>",
    "license": "GPL-3.0-or-later",
    "private": true,
    "homepage": "",
    "browserslist": [
        "defaults",
        "maintained node versions",
        "> 5%"
    ],
    "scripts": {
        "start": "npx @11ty/eleventy --serve",
        "clean": "rimraf docs",
        "serve:tailwind": "postcss src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
        "build:tailwind": "NODE_ENV=production postcss src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
        "debug": "DEBUG=* npx eleventy",
        "prettier": "npx prettier --write '.'",
        "prestart": "npm run clean && npm run serve:tailwind",
        "build": "npm run clean build:vite && npm run build:eleventy",
        "build:eleventy": "NODE_ENV=production eleventy",
        "build:vite": "NODE_ENV=production vite build",
        "prod": "NODE_ENV=production npm run build && serve docs",
        "dev": "npm run prestart && ELEVENTY_ENV=dev npx eleventy --serve"
    },
    "devDependencies": {
        "@11ty/eleventy": "^1.0.1",
        "@11ty/eleventy-navigation": "^0.3.3",
        "@11ty/eleventy-plugin-rss": "^1.2.0",
        "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
        "@tailwindcss/typography": "^0.5.2",
        "@vitejs/plugin-legacy": "^1.8.2",
        "autoprefixer": "^10.4.7",
        "cssnano": "^5.1.12",
        "dayjs": "^1.11.3",
        "eleventy-plugin-lazyimages": "^2.1.2",
        "eleventy-plugin-youtube-embed": "^1.7.0",
        "html-minifier": "^4.0.0",
        "markdown-it": "^13.0.1",
        "markdown-it-anchor": "^8.6.4",
        "postcss": "^8.4.14",
        "postcss-cli": "^9.1.0",
        "rimraf": "^3.0.2",
        "tailwindcss": "^3.1.4",
        "vite": "^2.9.12"
    },
    "dependencies": {
        "@iktakahiro/markdown-it-katex": "^4.0.1",
        "eleventy-plugin-footnotes": "^0.10.0",
        "serve": "^13.0.2"
    }
}
